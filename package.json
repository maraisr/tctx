{
	"name": "tctx",
	"version": "0.0.11",
	"description": "W3C Trace Contexts made simple",
	"keywords": [
		"tracecontext",
		"traceparent",
		"distributed",
		"tracing",
		"w3c"
	],
	"repository": "maraisr/tctx",
	"license": "MIT",
	"author": "Marais Rossow <me@marais.dev> (https://marais.io)",
	"sideEffects": false,
	"exports": {
		".": {
			"import": "./index.mjs",
			"require": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "./index.js",
	"module": "./index.mjs",
	"types": "index.d.ts",
	"files": [
		"index.mjs",
		"index.js",
		"index.d.ts"
	],
	"scripts": {
		"bench": "node -r tsm bench/index.ts",
		"build": "bundt --minify",
		"format": "prettier --write --list-different \"{*,src/*,test/*,bench/*,.github/**/*}.+(ts|js|json|yml|md)\"",
		"test": "uvu test \".spec.ts$\" -r tsm",
		"typecheck": "tsc --noEmit --skipLibCheck"
	},
	"prettier": "@marais/prettier",
	"dependencies": {
		"@lukeed/csprng": "^1.0.1"
	},
	"devDependencies": {
		"@marais/prettier": "0.0.1",
		"@marais/tsconfig": "0.0.3",
		"@types/node": "18.14.6",
		"bundt": "2.0.0-next.2",
		"prettier": "2.8.4",
		"tsm": "2.3.0",
		"typescript": "4.9.5",
		"uvu": "0.5.2"
	},
	"engines": {
		"node": ">=14"
	},
	"volta": {
		"node": "17.9.0"
	}
}
